	<xs:element name="IndberetningTilbagemeldingStruktur" type="ekap2020:IndberetningTilbagemeldingStrukturType"/>
	<xs:complexType name="IndberetningTilbagemeldingStrukturType">
		<xs:sequence>
			<xs:element name="Indberetningsperiode">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ekap2020:IndkomstÅr"/>
