				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ekap2020:MasseindleveringReference" minOccurs="0"/>
						<xs:element ref="ekap2020:IndberetningID" minOccurs="0"/>
						<xs:element ref="ekap2020:IndberetningValideringStatus"/>
						<xs:element name="AntalKontohavere">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Total">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ekap2020:OptællingAntal"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="MedFejl">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ekap2020:OptællingAntal"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Godkendt">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Total">
													<xs:complexType>
														<xs:sequence>
															<xs:element ref="ekap2020:OptællingAntal"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="UdenAdvis">
													<xs:complexType>
														<xs:sequence>
															<xs:element ref="ekap2020:OptællingAntal"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="MedAdvis">
													<xs:complexType>
														<xs:sequence>
															<xs:element ref="ekap2020:OptællingAntal"/>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="FejlListe">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Fejl" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ekap2020:IndberetningFejlNummer"/>
												<xs:element ref="ekap2020:IndberetningFejlTekst"/>
												<xs:element name="IdentifikationFeltListe" minOccurs="0">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Felt" minOccurs="1" maxOccurs="unbounded">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element ref="ekap2020:IndberetningValideringIdentifikation"/>
																		<xs:element ref="ekap2020:IndberetningValideringIdentifikationVærdi" minOccurs="0"/>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AdvisListe">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Advis" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ekap2020:IndberetningAdvisNummer"/>
												<xs:element ref="ekap2020:IndberetningAdvisTekst"/>
												<xs:element name="IdentifikationFeltListe" minOccurs="0">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Felt" minOccurs="1" maxOccurs="unbounded">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element ref="ekap2020:IndberetningValideringIdentifikation"/>
																		<xs:element ref="ekap2020:IndberetningValideringIdentifikationVærdi" minOccurs="0"/>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="KontohaverGodkendtListe">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="IdentifikationKontohaver" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ekap2020:IndberetningValideringIdentifikation"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
